<div class="row">
    <div class="col-lg-12">
        <h1 style="padding-bottom: 9px;margin: 10px 0 20px;border-bottom: 1px solid #eee;">Node List</h1>
    </div>
    <div class="col-lg-12">
    <div class="table-responsive">    
    <table class="table table-hover table-bordered">
      <thead>
        <tr>
          <th>title</th>
          <th>summary</th>
          <th>due_date</th>
        </tr>
      </thead>
      <tbody>
          {{#each notes_list}} 
        <tr>
          <td>{{title}}</td>
          <td><a href="/{{index}}">{{{summary}}}</a></td>
          <td>{{due_date}}</td>
        </tr>
         {{/each}}
        
      </tbody>
    </table>
    </div>
  </div>
        
</div>