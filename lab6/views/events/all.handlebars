<table class="table table-striped bs-example">
    <thead>
    <tr>
      <th>id</th>
      <th>attendees</th>
      <th>title</th>
      <th>startTime</th>
      <th>description</th>
      <th>location</th>
    </tr>
  </thead>

  <tbody>
        {{#each events}} 
        <tr>
        <td><a href="/events/{{id}}">{{id}}</a></td>
        <td>{{#each peopleArr}}<a href="/people/{{id}}">{{name}}</a><br/>{{/each}}</td>
        <td>{{title}}</td>
        <td>{{startTime}}</td>
        <td>{{description}}</td>
        <td>{{#each locationsArr}}<a href="/locations/{{id}}">{{name}}</a><br/>{{/each}}</td>
        </tr>
        {{/each}}
  </tbody>
</table>
